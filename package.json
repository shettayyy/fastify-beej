{
  "name": "fastify-beej",
  "version": "0.0.1",
  "description": "A boilerplate to create GraphQL APIs using fastify.",
  "main": "index.js",
  "repository": "https://github.com/rashtay/fastify-beej.git",
  "author": "Rahul Shetty <shettyrahul8june@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "docker:build": "docker build -t rashtay/fastify-beej .",
    "docker:run": "docker run rashtay/fastify-beej",
    "docker:scan": "docker scan rashtay/fastify-beej",
    "docker:sh": "docker run -it rashtay/fastify-beej sh"
  },
  "dependencies": {
    "fastify": "^3.20.2"
  }
}
